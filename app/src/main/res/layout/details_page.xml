<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/button"
        android:layout_width="165dp"
        android:layout_height="73dp"
        android:text="Export Finance"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="498dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="165dp"
        android:layout_height="73dp"
        android:text="Sell"
        tools:layout_editor_absoluteX="220dp"
        tools:layout_editor_absoluteY="498dp"
        />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="401dp"
        android:layout_height="86dp"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="1dp">

        <EditText
            android:id="@+id/editTextTextPersonName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="31dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="MmK species"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageView2"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="70dp"
            android:layout_height="56dp"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="16dp"
            tools:srcCompat="@tools:sample/avatars" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="49dp"
            android:layout_height="52dp"
            app:srcCompat="@drawable/ic_baseline_menu_24"
            tools:layout_editor_absoluteX="97dp"
            tools:layout_editor_absoluteY="17dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="411dp"
        android:layout_height="167dp"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteY="78dp"
        tools:srcCompat="@tools:sample/backgrounds/scenic" />

    <View
        android:id="@+id/divider2"
        android:layout_width="409dp"
        android:layout_height="1dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteY="587dp" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="101dp"
        android:layout_height="88dp"
        app:srcCompat="@android:drawable/stat_sys_phone_call"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="606dp" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="103dp"
        android:layout_height="87dp"
        app:srcCompat="@drawable/ic_baseline_email_24"
        tools:layout_editor_absoluteX="164dp"
        tools:layout_editor_absoluteY="608dp" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="92dp"
        android:layout_height="88dp"
        app:srcCompat="@android:drawable/sym_action_chat"
        tools:layout_editor_absoluteX="290dp"
        tools:layout_editor_absoluteY="606dp" />

    <LinearLayout
        android:layout_width="409dp"
        android:layout_height="238dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="-4dp"
        tools:layout_editor_absoluteY="260dp">

        <TextView
            android:id="@+id/textView6"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:lineSpacingMultiplier="1.5"
            android:text="\t\t\tContact person Name: Ms Anjum \n
      Categories: Spices, Rice, wheat \n
      Date of Incorporation: XYZZYXZX \n
      Importer Credit Ranking: High \n
      Contact Time preference: 9AM - 10AM Local Time \n
      Company Phone Number: +14454656465 \n
      Contact Personal Number: +14464243466 \n
      Conact person Email ID: xyz@abc.com"
            android:textSize="16dp" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>